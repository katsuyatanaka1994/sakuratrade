# Development defaults. Copy to .env for local work.
# PostgreSQL only. If your username contains spaces, URL-encode them (example: katsuya%20tanaka).
ENV=development
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/gptset_dev
LOG_LEVEL=INFO
OPENAI_API_KEY=replace-me
DB_POOL_PRE_PING=true
DB_POOL_RECYCLE=1800
DB_POOL_TIMEOUT=30
DATABASE_ECHO=false
MOCK_AI=false
JWT_SECRET_KEY=change-me
